<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hqs.test.dao.TestDao">
    <select id="getTest" parameterType="int" resultType="Test">
        SELECT id, name
        FROM test
        WHERE id = #{id}
    </select>
</mapper>